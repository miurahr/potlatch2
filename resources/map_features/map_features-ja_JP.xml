<?xml version="1.0" encoding="UTF-8"?>
<!--
	See http://wiki.openstreetmap.org/wiki/Potlatch_2/Developer_Documentation/Map_Features for documentation.
	Files can be included like this: <include file="map_features/roads.xml">
-->

<mapFeatures>

  <!-- Categories -->
  <category name="道路" id="roads"/><!-- don't change id! for localize -->
  <category name="Paths" id="paths"/>
  <category name="ショッピング" id="shopping"/>
  <category name="飲食" id="foodanddrink"/>
  <category name="Amenity" id="amenity"/>
  <category name="観光" id="tourism"/>
  <category name="宿泊施設" id="accommodation"/>
  <category name="交通機関" id="transport"/>
  <category name="河川" id="water"/>
  <category name="自然" id="natural"/>
  <category name="障害物" id="barrier"/>
  <category name="Power" id="power"/>
  <category name="役所" id="admin"/>
  <category name="建物" id="buildings"/>
  <category name="土地利用" id="landuse"/>
  <category name="農業" id="agriculture"/>
  <category name="高度" id="advanced"/>
  <category name="場所" id="places"/>
  <category name="スポーツとレジャー" id="sport"/>
  <category name="人工物" id="manmade"/>

  <!-- Common input sets -->
  
  <!-- you can localize name, description, category and text. don't touch ref, id, key, value, priority, type, presence -->

  <inputSet id="simpleName"><!-- don't change id! -->
    <input type="freetext" presence="always" name="名称" key="name" description="名称" priority="highest"/>
  </inputSet>
  
  <inputSet id="nameAndRef">
    <inputSet ref="simpleName" />
    <input type="freetext" presence="always" name="参照番号" key="ref" description="地物を参照する番号やコード" />
  </inputSet>
 
  <inputSet id="source">
    <input type="freetext" category="詳細" presence="onTagMatch" name="情報源" key="source" description="この地物入力の主たる情報源(GPS, 測量, Yahoo, ...)" priority="lowest"/>
  </inputSet>

  <inputSet id="designation">
    <input type="freetext" category="詳細" presence="onTagMatch" description="Official designation or classification" name="Designation" key="designation"/>
  </inputSet>

  <inputSet id="common">
    <inputSet ref="source"/>
    <inputSet ref="designation"/>
  </inputSet>

  <inputSet id="names">
    <input type="freetext" presence="always"
        name="名称" category="詳細" priority="highest"
        key="name" description="一般的な名称"/>
    <input type="freetext" presence="onTagMatch"
        name="国際的な名称" category="詳細"
        key="int_name" description="国際的に理解できる名称"/>
    <input type="freetext" presence="onTagMatch"
        name="歴史的名称" category="詳細" priority="low"
        key="old_name" description="歴史的なあるいは、以前の名称"/>
    <input type="freetext" presence="onTagMatch"
        name="別称" category="詳細" priority="low"
        key="alt_name" description="別の現在利用されている名称"/>
  </inputSet>

  <inputSet id="wifi">
    <input type="choice" presence="onTagMatch" category="詳細" name="無線LANサービス" key="wifi">
      <choice value="free" text="Free"/>
      <choice value="yes" text="Yes"/>
      <choice value="no" text="No"/>
    </input>
  </inputSet>

  <inputSet id="buildingAddress">
    <input type="freetext" presence="onTagMatch" category="住所" description="The number of the house, e.g. 156 or 10-12" name="部屋番号" key="addr:housenumber"/>
    <input type="freetext" presence="onTagMatch" category="住所" description="The name of the house, e.g. Riverbank Cottage" name="建物名称" key="addr:housename"/>
    <input type="freetext" presence="onTagMatch" category="住所" description="The Street Name (optional)" name="Street Name" key="addr:street"/>
    <input type="freetext" presence="onTagMatch" category="住所" description="The postcode" name="郵便番号" key="addr:postcode"/>
  </inputSet>

  <inputSet id="web">
    <input type="freetext" presence="onTagMatch" category="Address" description="WebサイトのURL" name="Webサイト" key="website"/>
  </inputSet>

  <inputSet id="cuisine">
    <input type="choice" presence="always" name="料理の種別" category="詳細" description="レストランのタイプ" key="cuisine">
      <!-- The 30 most popular values according to taginfo 23/12/2010 -->
      <choice value="burger" text="ハンバーガー"/>
      <choice value="chicken" text="鶏料理"/>
      <choice value="chinese" text="中華"/>
      <choice value="coffee_shop" text="コーヒー"/>
      <choice value="greek" text="ギリシャ料理"/>
      <choice value="pizza" text="ピザ"/>
      <choice value="sandwich" text="サンドウイッチ"/>
      <choice value="seafood" text="シーフード"/>
      <choice value="regional" text="Regional"/>
      <choice value="italian" text="イタリアン料理"/>
      <choice value="german" text="ドイツ料理"/>
      <choice value="kebab" text="ケバブ・ギョロ"/>
      <choice value="indian" text="インド料理"/>
      <choice value="asian" text="アジア料理"/>
      <choice value="mexican" text="メキシコ料理"/>
      <choice value="thai" text="タイ料理"/>
      <choice value="japanese" text="和食"/>
      <choice value="ice_cream" text="アイスクリーム"/>
      <choice value="fish_and_chips" text="フィッシュ &amp; チップス"/>
      <choice value="turkish" text="トルコ料理"/>
      <choice value="french" text="フランス料理"/>
      <choice value="sushi" text="寿司"/>
      <choice value="american" text="アメリカ料理"/>
      <choice value="steak_house" text="ステーキハウス"/>
      <choice value="international" text="国際料理"/>
      <choice value="spanish" text="スペイン料理"/>
      <choice value="vietnamese" text="ベトナム料理"/>
      <choice value="fish" text="魚料理"/>
      <choice value="bavarian" text="Bavarian"/>
      <choice value="vegetarian" text="ベジタリアン"/>

      <help>http://wiki.openstreetmap.org/wiki/Key:cuisine</help>
    </input>
  </inputSet>

  <!-- Roads -->

  <inputSet id="majorRoad">
    <inputSet ref="names"/>
    <inputSet ref="roadRefs"/>
    <inputSet ref="roadRestrictions"/>
    <inputSet ref="roadPhysical"/>
    <inputSet ref="cycle"/>
    <inputSet ref="bicycle-lane"/>
    <inputSet ref="bus-route"/>
    <inputSet ref="pedestrians"/>
    <inputSet ref="roadLanes"/>
    <inputSet ref="roadRoundabout"/>
    <inputSet ref="permissions"/>
  </inputSet>
  

  <inputSet id="minorRoad">
    <inputSet ref="names"/>
    <inputSet ref="roadRestrictions"/>
    <inputSet ref="roadPhysical"/>
    <inputSet ref="cycle"/>
    <inputSet ref="bicycle-lane"/>
    <inputSet ref="bus-route"/>
    <inputSet ref="pedestrians"/>
    <inputSet ref="roadLanes"/>
    <inputSet ref="roadRoundabout"/>
    <inputSet ref="permissions"/>
  </inputSet>

  <inputSet id="path">
    <inputSet ref="simpleName"/>
    <inputSet ref="roadPhysical"/>
    <inputSet ref="cycle"/>
    <inputSet ref="ldp"/>
    <inputSet ref="pedestrians"/>
    <inputSet ref="permissions"/>
  </inputSet>

  <inputSet id="junctionNode">
    <inputSet ref="turnRestrictions"/>
  </inputSet>

  <inputSet id="roadRefs">
    <input type="freetext" presence="always"
        name="道路番号" category="詳細"  priority="high"
        key="ref" description="公式の道路番号"/>
    <input type="freetext" presence="onTagMatch"
        name="国際道路番号" category="詳細" subcategory="Additional names" 
        key="int_ref" description="公式な国際的な道路番号"/>
    <input type="freetext" presence="onTagMatch"
        name="古い道路番号" category="詳細" subcategory="Additional names" priority="low"
        key="old_ref" description="歴史的なあるいは、以前の道路番号"/>
  </inputSet>

  <inputSet id="roadPhysical">
    <input type="freetext" presence="onTagMatch"
        name="幅" category="詳細" subcategory="Physical"
        key="width" description="道路幅" layout="horizontal"/>
    <input type="choice" presence="onTagMatch"
        name="道路表面" category="詳細" description="路面のタイプ"
        key="surface" layout="horizontal">
      <choice value="unpaved" text="未舗装" description="Road surface is unsealed"/>
      <choice value="paved" text="舗装" description="Road surface is sealed"/>
      <choice value="asphalt" text="アスファルト"/>
      <choice value="concrete" text="コンクリート"/>
      <choice value="paving_stones" text="敷石"/>
      <choice value="cobblestone" text="Cobblestone"/>
      <choice value="sand" text="砂"/>
      <choice value="gravel" text="Gravel"/>
      <choice value="dirt" text="ダートロード"/>
      <choice value="grass" text="草地"/>
    </input>
    <inputSet ref="bridge"/>
    <inputSet ref="tunnel"/>
    <inputSet ref="embankment-cutting"/>
    <!-- not sure which category best suits put area=yes -->
    <input type="checkbox" presence="onTagMatch" category="詳細" subcategory="Physical" key="area" name="Open area" description="The way is a large open space, like at a dock, where vehicles can move anywhere within the space, rather than just along the edge." />
  </inputSet>

  <inputSet id="roadLanes">
    <input presence="onTagMatch" type="number" name="レーン" category="詳細" description="Total number of lanes, counting both directions"
           key="lanes" minimum="1" maximum="10" layout="horizontal"/>
  </inputSet>

  <inputSet id="bridge">
    <input type="choice" presence="onTagMatch"
        name="Bridge" category="詳細" description="Road goes over a bridge"
        key="bridge" layout="horizontal">
      <choice value="yes" text="一般的な橋" description="一般的な橋 -- タイプ不明"/>
      <choice value="viaduct" text="Viaduct" description="Viaduct"/>
      <choice value="suspension" text="吊り橋"/>
    </input>
    <input type="slider" presence="onTagMatch"
        name="Layer" category="詳細" description="相対的な高低の位置 (-5　最低, +5 最高)"
        key="layer" minimum="-5" maximum="5" default="0" snapInterval="1" labels="Lowest,Ground,Highest"
        defaultName="Ground"/>
  </inputSet>
  <inputSet id="tunnel">
    <!-- Not ideal, used for non-roads too. -->  
    <input type="choice" presence="onTagMatch"
        name="トンネル" category="詳細"subcategory="Physical" description="道路はトンネルの中にある"
        key="tunnel" layout="horizontal">
      <choice value="yes" text="トンネル" description="一般的なトンネル"/>
    </input>
  </inputSet>

  <inputSet id="embankment-cutting">
    <input type="choice"
           name="Embankment" category="詳細" subcategory="Physical" description="Road supported on a raised bed of earth and rock."
           key="embankment" layout="horizontal">
      <choice value="yes" text="Embankment"/>
    </input>
    <input type="choice"
           name="Cutting" category="詳細" subcategory="Physical" description="Road carved out of hill on one or both sides."
           key="cutting" layout="horizontal">
      <choice value="yes" text="Cutting"/>
    </input>

  </inputSet>

  <inputSet id="rail-electrification">
    <input type="choice" name="Electrified" category="詳細" subcategory="Electrification" description="Is the track electrified (whether by 3rd rail, overhead wires, etc)?"
           key="electrified">
      <choice value="yes" text="Yes"/>
      <choice value="contact_line" text="Overhead line"/>
      <choice value="rail" text="Third rail"/>
      <choice value="no" text="No"/>
    </input>
    <input type="choice" name="Voltage" category="詳細" subcategory="Electrification" description="Nominal voltage of electric wires"
           key="voltage" presence="withCategory">
      <choice value="600" text="600V"/>
      <choice value="750" text="750V"/>
      <choice value="1500" text="1500V"/>
      <choice value="3000" text="3000V"/>
      <choice value="15000" text="15kV"/>
      <choice value="25000" text="25kV"/>
    </input>
    <input type="choice" name="Frequency" category="詳細" subcategory="Electrification" description="Frequency in Hertz of alternating current power supply"
           key="frequency" presence="withCategory">
      <choice value="0" text="DC"/>
      <choice value="16.67" text="16.67 Hz"/>
      <choice value="16.7" text="16.7 Hz"/>
      <choice value="25" text="25 Hz"/>
      <choice value="50" text="50 Hz"/>
      <choice value="60" text="60 Hz"/>
    </input>
  </inputSet>

  <inputSet id="fee">
    <input type="freetext" presence="onTagMatch" category="詳細" subcategory="Restrictions" description="The charge/cost of using this amenity" name="Fee" key="fee"/>
  </inputSet>

  <inputSet id="roadRestrictions">
    <input type="choice" presence="always"
        name="Oneway" category="通行制限" description="Oneway roads"
        key="oneway">
      <choice value="yes" match="yes|true|1" text="一方通行"
        description="この道路は一方通行でのみ通行可能" icon="features/oneway__yes.png"/>
      <choice value="no" match="no|false|0" text="両方向通行"
        description="この道路は、両方向に通行可能" icon="features/oneway__no.png"/>
      <choice value="-1" match="-1|reverse" text="One way reverse"
        description="この道路は、逆方向に通行可能" icon="features/oneway__-1.png"/>
    </input>
    <input type="speed" presence="onTagMatch"
        name="制限速度" category="通行制限" description="この道路の最大速度"
        key="maxspeed"/>
  </inputSet>

  <inputSet id="roadRoundabout">
    <!-- review the choice of category -->
    <input type="choice" presence="onTagMatch" name="Roundabout" category="Restrictions" description="Whether this road is a roundabout. Make the way face the direction appropriate for the country."
           key="junction">
      <choice value="roundabout" text="Yes"/>
    </input>
  </inputSet>

  <inputSet id="turnRestrictions">
    <input type="turn" name="方向転換制限" description="方向転換制限" category="通行制限" priority="normal" presence="onTagMatch">
      <match k="type" v="restriction"/>
      <role role="via"/>
    </input>
  </inputSet>

  <inputSet id="pedestrians">
    <input type="choice" name="通行人" description="通行人がこの道路を使えるか、歩道を含む" category="歩行" key="foot">
      <choice value="yes" text="許可"/>
      <choice value="no" text="禁止"/>
      <choice value="designated" text="ルール"/>
    </input>
  </inputSet>

  <inputSet id="naptan">
    <input type="freetext" presence="onTagMatch" category="交通" description="12 character internal Naptan ID" name="Atco Code" key="naptan:AtcoCode"/>
    <input type="choice" presence="onTagMatch" category="交通" description="The eight-point compass bearning" name="Naptan Bearing" key="naptan:Bearing" >
      <choice value="N"  match="N"  text="北"  description=""/>
      <choice value="NE" match="NE" text="北東" description=""/>
      <choice value="E"  match="E"  text="東"  description=""/>
      <choice value="SE" match="SE" text="南東" description=""/>
      <choice value="S"  match="S"  text="南"  description=""/>
      <choice value="SW" match="SW" text="南西" description=""/>
      <choice value="W"  match="W"  text="西"  description=""/>
      <choice value="NW" match="NW" text="北西" description=""/>
    </input>
    <input type="freetext" presence="onTagMatch" category="交通" description="The naptan common name" name="Naptan Common Name (read-only)" key="naptan:CommonName"/>
    <input type="freetext" presence="onTagMatch" category="交通" description="" name="Naptan Indicator (read-only)" key="naptan:Indicator"/>
    <input type="freetext" presence="onTagMatch" category="交通" description="" name="Naptan Street (read-only)" key="naptan:Street"/>
    <input type="freetext" presence="onTagMatch" category="交通" description="Delete this when the details have been verified on-the-ground" name="Naptan Verified?" key="naptan:verified"/>
  </inputSet>

  <inputSet id="buses">
    <input type="freetext" presence="always" category="交通" name="Stop Name" key="name" description="The name of the bus stop"/>
    <input type="freetext" presence="always" category="交通" name="Local Ref" key="local_ref" description="The local reference of the stop, usually one or two letters above the main flag, used at bus interchanges, e.g. L, BX"/>
    <inputSet ref="naptan"/>
  </inputSet>

  <inputSet id="bus-route">
    <input type="route" name="Bus Route" description="Bus route" category="Transport" priority="low" presence="onTagMatch">
      <match k="type" v="route"/>
      <match k="route" v="bus"/>
      <icon image="features/route__bus.png">
        <font size="12pt">${operator} <b>${ref}</b></font>
      </icon>
    </input>
  </inputSet>

  <inputSet id="tram-route">
    <input type="route" name="Tram Route" description="Tram route" category="Transport" priority="low" presence="onTagMatch">
      <match k="type" v="route"/>
      <match k="route" v="tram"/>
      <icon image="features/transport__tram.png">
        <font size="12pt">${operator} <b>${ref}</b></font>
      </icon>
    </input>
  </inputSet>

  <inputSet id="cycle">
    <inputSet ref="bicycle-permission"/>
    <input type="route" name="National Cycle Routes" description="A signposted route in a National Cycle Network, or nearest equivalent." category="Cycle" priority="low">
      <match k="type" v="route"/>
      <match k="route" v="bicycle"/>
      <match k="network" v="ncn"/>
      <icon image="features/route__ncn.png" background="#ff6f7a" foreground="white">
        <font size="14pt"><b>${ref}</b></font><br/>
        <font size="12pt">${name}</font>
      </icon>
    </input>
    <input type="route" name="Regional Cycle Routes" description="A signposted route in a Regional Cycle Network, or nearest equivalent." category="Cycle" priority="low">
      <match k="type" v="route"/>
      <match k="route" v="bicycle"/>
      <match k="network" v="rcn"/>
      <icon image="features/route__rcn.png" background="#6ff7ff" foreground="white">
        <font size="14pt"><b>${ref}</b></font><br/>
        <font size="12pt">${name}</font>
      </icon>
    </input>
    <input type="route" name="Local Cycle Routes" description="A signposted route in a Local Cycle Network, or nearest equivalent." category="Cycle" priority="lowest">
      <match k="type" v="route"/>
      <match k="route" v="bicycle"/>
      <match k="network" v="lcn"/>
      <icon image="features/route__lcn.png" background="#7d6fff" foreground="white">
        <font size="14pt"><b>${ref}</b></font><br/>
        <font size="12pt">${name}</font>
      </icon>
    </input>
  </inputSet>

  <inputSet id="bicycle-permission">
    <input type="choice"
           name="Bicycles permitted" category="Cycle" description="Are bicyles allowed to use this road (regardless of physical suitability)?"
           key="bicycle">
      <choice value="yes" text="Allowed"/>
      <choice value="no" text="Prohibited" desciption="Cycling is not allowed. Pushing your bicycle is not allowed either."/>
      <choice value="private" text="Private"/>
      <choice value="dismount" text="Cyclists dismount" description="Signage states that cyclists should dismount and push their bike for the selected path or road."/>
      <choice value="designated" text="Designated"/>
      <help>http://wiki.openstreetmap.org/wiki/Key:access</help>
    </input>
  </inputSet>

  <inputSet id="bicycle-lane">
    <input type="choice"
        name="Bike lanes" category="Cycle" description="Road has bike lanes within the road surface"
        key="cycleway" layout="horizontal">
      <choice value="no" text="No bike lanes"/>
      <choice value="lane" text="On-road bike lane" description="Separated by painted line on the road from cars"/>
      <choice value="track" text="Parallel track" description="Separated by kerb or parked cars"/>
      <choice value="opposite_lane" text="Contraflow lane" description="Separated by painted line, and allowing bicycles in both directions in an otherwise one-way street."/>
      <choice value="opposite_track" text="Contraflow track" description="Separated by kerb or parked cars, and allowing bicycles in both directions in an otherwise one-way street."/>
      <choice value="opposite" text="Contraflow unmarked" description="The route may be cycled in the direction opposite of other traffic, but does not have a dedicated lane."/>
      <help>http://wiki.openstreetmap.org/wiki/Key:cycleway</help>
    </input>
  </inputSet>

  <inputSet id="ldp">
    <input type="route" name="National Walking Route" description="National walking route" category="Walk" priority="normal">
      <match k="type" v="route"/>
      <match k="route" v="hiking|foot"/>
      <match k="network" v="nwn"/>
      <icon image="features/route__nwn.png" background="red" foreground="white">
        <font size="14pt"><b>${ref}</b></font><br/>
        <font size="12pt">${name}</font>
      </icon>
    </input>
    <input type="route" name="Regional Walking Route" description="Regional walking route" category="Walk" priority="low">
      <match k="type" v="route"/>
      <match k="route" v="hiking|foot"/>
      <match k="network" v="rwn"/>
      <icon image="features/route__rwn.png" background="cyan" foreground="white">
        <font size="14pt"><b>${ref}</b></font><br/>
        <font size="12pt">${name}</font>
      </icon>
    </input>
    <input type="route" name="Local Walking Route" description="Local walking route" category="Walk" priority="lowest">
      <match k="type" v="route"/>
      <match k="route" v="hiking|foot"/>
      <match k="network" v="lwn"/>
      <icon image="features/route__lwn.png" background="blue" foreground="white">
        <font size="14pt"><b>${ref}</b></font><br/>
        <font size="12pt">${name}</font>
      </icon>
    </input>
  </inputSet>

  <inputSet id="route">
    <inputSet ref="names"/>
    <inputSet ref="roadRefs"/>
  </inputSet>

  <inputSet id="waterways">
    <inputSet ref="simpleName"/>
    <input type="freetext" presence="always" name="Width" category="Properties" key="width" description="The width in metres"/>
    <inputSet ref="boatPermissions"/>
  </inputSet>
<!-- Would be good to have a dedicated 'access' type of input -->
  <!-- all the description fields are identical at the moment for ease of maintenance, should be tailored in future. -->
  <inputSet id="boatPermissions">
    <input type="choice" name="ボートの許可" category="通行制限" key="boat" description="Are boats allowed to use this waterway?">
      <choice value="yes" text="Allowed" description="General right of way."/>
      <choice value="no" text="Prohibited" description="No access to the public."/>
      <choice value="permissive" text="Permissive" description="Access permitted through private land."/>
      <choice value="private" text="Private" description="No access to the public, except individual exceptions."/>
      <choice value="designated" text="Designated" description="Permitted, according to signs or specific local laws."/>
    </input>
  </inputSet>

  <inputSet id="permissions">
    <input type="choice" name="一般通航" category="通行制限" key="access" description="Is there a general right of access, regardless of mode of transport?">
      <choice value="yes" text="Allowed" description="General right of way."/>
      <choice value="no" text="Prohibited" description="No access to the public."/>
      <choice value="permissive" text="Permissive" description="Access permitted through private land."/>
      <choice value="private" text="Private" description="No access to the public, except individual exceptions."/>
      <choice value="designated" text="Designated" description="Permitted, according to signs or specific local laws."/>
    </input>

    <input type="choice" name="自動車" category="通行制限" key="motor_vehicle" description="Are cars and other private vehicles allowed?">
      <choice value="yes" text="Allowed" description="General right of way."/>
      <choice value="no" text="Prohibited" description="No access to the public."/>
      <choice value="permissive" text="Permissive" description="Access permitted through private land."/>
      <choice value="private" text="Private" description="No access to the public, except individual exceptions."/>
      <choice value="designated" text="Designated" description="Permitted, according to signs or specific local laws."/>
    </input>

    <input type="choice" name="Horses" category="通行制限" key="horse" description="Are cars and other private vehicles allowed?">
      <choice value="yes" text="許可" description="一般に通行可能"/>
      <choice value="no" text="禁止" description="公衆は通行できない"/>
      <choice value="permissive" text="許諾" description="私道の通行が許諾されている"/>
